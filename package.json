  "bugs": {
    "url": "https://github.com/jpelton-stroud/gas-generic-starter/issues"
  },
  "homepage": "https://github.com/jpelton-stroud/gas-generic-starter#readme",
  "devDependencies": {
    "@google/clasp": "^3.1.3",
    "@types/google-apps-script": "^2.0.7",
    "cpy": "^12.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  }
}
